<body ng-app="OpenPO" ng-controller="HomeController">

    <header id="header">
        <div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4  fadeInDown one">
                <a href="" class="logo">

                    @*<img src="~/Content/Assets/management.svg" style=" float: left; width: 3em;" />*@
                    <strong style="float: left;font-family:'Segoe UI'">
                        <i class="fa fa-bars openleftnav" ng-click="openLeftMenu()"></i>   Open PO Report
                    </strong>
                </a>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4 text-center fadeInDown two">
                <img src="~/Content/Assets/logo.png" style=" width: 3em;max-width: 40px;" />
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4  fadeInDown three">
                @*<nav>
                        <a href="#menu">Menu</a>
                    </nav>*@
                <strong style="float: right;font-family:'Segoe UI';color:#fff">  {{loggedinuser}} <img style=" border-radius: 50%;" ng-src="{{imageurl}}" /></strong>
            </div>
        </div>



    </header>
    <div id="sideNavContainer"
         layout="row" ng-cloak>
        <md-sidenav md-component-id="left" class="md-sidenav-left">

            <nav id="menu1" style="margin-top:24%;">
